	"SFX\\SCP\\035\\KilledEscape.ogg": [
		{
			"text": "I commemorate you for your help. But I have no further business with you.",
			"delay": 0.5,
			"color": "035"
		},
		{
			"text": "I have no intentions of taking a person who «earned» their imprisonment with me.",
			"delay": 4.9,
			"color": "035"
		},
		{
			"text": "But for the sake of my freedom,",
			"delay": 10.6,
			"color": "035"
		},
		{
			"text": "The simpliest way for you to get out safely is to go and find SCP-012 in the Light Containment Zone.",
			"delay": 12.7,
			"color": "035"
		},
		{
			"text": "If you can read between the lines,",
			"delay": 19.0,
			"color": "035"
		},
		{
			"text": "it'll be as interpretable as a map.",
			"delay": 21.7,
			"color": "035"
		},
