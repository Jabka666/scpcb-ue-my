	"SFX\\SCP\\035\\GasedEscape.ogg": [
		{
			"text": "Unfortunately, I can't take you with me.",
			"delay": 0.39,
			"color": "035"
		},
		{
			"text": "You're too much of a liability in many ways. So yes, I did lie to you.",
			"delay": 3.35,
			"color": "035"
		},
		{
			"text": "But for the sake of my freedom,",
			"delay": 8.5,
			"color": "035"
		},
		{
			"text": "The easiest way to get out safely is probably Gate A.",
			"delay": 10.55,
			"color": "035"
		},
		{
			"text": "The security is pretty high, but if you manage to sneak past the guards and reach the lower level under the bridge, there's an unguarded service tunnel.",
			"delay": 15.25,
			"color": "035"
		},
		{
			"text": "That's your way out.",
			"delay": 23.35,
			"color": "035"
		}
